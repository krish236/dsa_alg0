#include<bits/stdc++.h>
using namespace std;
int  partition(int arr[],int s,int e){
int i = s-1;
int piv = arr[e];
for(int j = s;j<e;j++){
  if(arr[j]<piv){
i++;
swap(arr[j],arr[i]);
  }
}
swap(arr[e],arr[i+1]);
return i+1;
}

void quickSort(int arr[],int n ,int s,int e){
  if(s>=e){
    return ;
  }
  int p = partition(arr,s,e);
  quickSort(arr,n,s,p-1);
  quickSort(arr,n,p+1,e);
}

int main(){
  int arr[]={3,4,1,6,2,7,5};
  int n = 7;
  int s = 0;
  int e = n-1;
quickSort(arr,n,s,e);
for(int i = 0;i<n;i++){
cout<<arr[i]<<" ";
  
}
return 0;
}
